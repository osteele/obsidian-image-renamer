{
  "name": "obsidian-image-renamer",
  "version": "1.0.0",
  "description": "Rename image files using AI-generated captions in Obsidian",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check ."
  },
  "keywords": [
    "obsidian",
    "plugin",
    "image",
    "rename",
    "ai",
    "caption"
  ],
  "author": "Oliver Steele",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@types/bun": "^1.2.20",
    "@types/node": "^24.3.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.9",
    "obsidian": "^1.8.7",
    "typescript": "^5.9.2"
  }
}
